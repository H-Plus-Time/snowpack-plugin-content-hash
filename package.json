{
  "name": "snowpack-plugin-content-hash",
  "version": "1.0.0",
  "author": "akejolin",
  "description": "A plugin for snowpack which will add a hash in the file name. The hash is based on the files content.",
  "license": "MIT",
  "main": "src/plugin.js",
  "scripts": {
    "build": "pika build",
    "deploy": "pika publish",
    "version": "npm run build",
    "lint": "eslint src"
  },
  "keywords": [
    "snowpack",
    "snowpack-plugin",
    "content-hash",
    "hash",
    "cache"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/akejolin/snowpack-plugin-content-hash.git"
  },
  "@pika/pack": {
    "pipeline": [
      [
        "@pika/plugin-ts-standard-pkg"
      ],
      [
        "@pika/plugin-build-node"
      ]
    ]
  },
  "devDependencies": {
    "@pika/pack": "^0.5.0",
    "@pika/plugin-build-node": "^0.9.2",
    "@pika/plugin-ts-standard-pkg": "^0.9.2",
    "eslint": "^7.8.1",
    "eslint-plugin-react": "^7.20.6",
    "snowpack": "^2.6.4",
    "typescript": "^3.9.7"
  },
  "dependencies": {
    "@babel/core": "^7.11.5",
    "crypto": "^1.0.1",
    "esprima": "^4.0.1",
    "recursive-readdir": "^2.2.2",
    "replace": "^1.2.0"
  }
}
